<%= javascript_include_tag 'application', 'https://code.highcharts.com/modules/drilldown.js' %>
<%= javascript_include_tag 'application', 'https://code.highcharts.com/modules/data.js' %>

<main class="uk-width-expand uk-position-relative">
  <div class="uk-panel uk-section uk-section-small uk-section-muted uk-container uk-container-expand" data-uk-height-viewport="offset-top: true">
    <h1 class="uk-heading-divider" style="font-size: 20px">Administrators Dashboard</h1>
    <div class="uk-child-width-1-1 uk-child-width-1-2@m uk-child-width-1-3@l" data-uk-grid>
      <div class="uk-width-1-1">
        <div id="photo_order_summary" class="uk-card uk-card-default uk-card-body uk-card-hover"></div>
      </div>
      <div class="uk-width-1-2">
        <div id="payments_summary" class="uk-card uk-card-default uk-card-body uk-card-hover uk-padding-remove"></div>
      </div>
      <div class="uk-width-1-2">
        <div class="uk-card uk-card-default uk-card-body">
          <p>Recent Customer Feed</p>
        </div>
      </div>
      <div>
        <div class="uk-card uk-card-default uk-card-body uk-card-secondary">
          <p>Current Customer Orders</p>
        </div>
      </div>
      <div>
        <div class="uk-card uk-card-default uk-card-body uk-card-secondary">
          <p>Customer Messages</p>
        </div>
      </div>
    </div>
  </div>
</main>